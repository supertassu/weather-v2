{
  "name": "weather-v2",
  "private": true,
  "version": "1.0.0",
  "description": "practice stuff",
  "main": "index.js",
  "scripts": {
    "test": "rm db/test.sqlite ; xo && NODE_ENV=test ava",
    "dev": "concurrently --names \"main,webpack,ava,xo\" \"babel-node index.js\" \"webpack --mode=\"development\" --watch\" \"ava -v --watch\" \"onchange '*.js' -i -- xo\"",
    "build": "webpack --mode=\"production\"",
    "start": "babel-node index.js"
  },
  "author": "tassu <hello@tassu.me>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^1.0.0-beta.3",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chart.js": "^2.7.2",
    "css-loader": "^0.28.11",
    "dayjs": "^1.5.24",
    "eslint-config-xo-react": "^0.16.0",
    "eslint-plugin-react": "^7.7.0",
    "prop-types": "^15.6.1",
    "react-chartjs-2": "^2.7.2",
    "style-loader": "^0.20.3",
    "supertest": "^3.0.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "xo": "^0.20.3"
  },
  "dependencies": {
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "kcors": "^2.2.1",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-send": "^4.1.3",
    "onchange": "^3.3.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "sequelize": "^4.37.6",
    "sqlite3": "^4.0.0"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "xo": {
    "extends": "xo-react",
    "rules": {
      "import/no-unassigned-import": {
        "allow": [
          "**/*.css"
        ]
      }
    }
  }
}
